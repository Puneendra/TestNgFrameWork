<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" thread-count="4" parallel="tests">
    <test verbose="2" preserve-order="true" name="chrome-Regression">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="testScripts.RegistrationScripts">
                <methods>
                    <include name="Fillregistration"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="firefox-Regression">
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="testScripts.RegistrationScripts">
                <methods>
                    <include name="Fillregistration"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>